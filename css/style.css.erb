    -moz-border-radius-bottomleft: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;}
.list li.hover:hover {background-color:#efefef; cursor:pointer;}

input[type=text],textarea { margin-left: 15px; margin-right: 15px; padding:5px; border: 1px solid #A8A8A8;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;font-size: 0.9em;font-weight: bold;}

input::-webkit-input-placeholder,textarea::-webkit-input-placeholder {color: #808080;}
input:-moz-placeholder,textarea:-moz-placeholder {color: #808080;}

