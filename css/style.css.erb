    -moz-border-radius: 5px;
    border-radius: 5px; }
.list li {background-color:#FFF;  display: block;padding:5px; border-top: 1px solid #A8A8A8;font-size: 0.9em;font-weight: bold;  }
.list li:first-child {border-top: 0px;-webkit-border-top-left-radius: 5px;
    -webkit-border-top-right-radius: 5px;
    -moz-border-radius-topleft: 5px;
    -moz-border-radius-topright: 5px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px; }
.list li:last-child {border-bottom: 1px solid #A8A8A8;-webkit-border-bottom-right-radius: 5px;
    -webkit-border-bottom-left-radius: 5px;
