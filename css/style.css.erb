section {width:50%; position:relative;background-color: #FFFFFF;}
.a { background-color:#CC0}
.b { background-color:#6C3;}

section,.head_section {-webkit-border-top-left-radius: 5px;-webkit-border-top-right-radius: 5px;-moz-border-radius-topleft: 5px;-moz-border-radius-topright: 5px;border-top-left-radius: 5px;border-top-right-radius: 5px;}

.head_section { height:30px; color:#ffffff; background: url(<%= asset_path("disign/head_section.png") %>) repeat 0 0;}
.shadow:before { content:''; display:block;height: 100%; width:9px;left:-9px; z-index:1; position: absolute; background: url(<%= asset_path("disign/section_shadow.png") %>) repeat-y 0 0;}

.head_section b,.head_section i { display:inline-block; width:30px; height:30px;}
.head_section b {float:left}
.head_section i {float:right;}
section.arrow_left b, section.arrow_right i {cursor:pointer;background: url(<%= asset_path("disign/arrow_section.png") %>) no-repeat 4px -31px;}
section.arrow_left b {background-position: 4px -92px;}

section.arrow_left b:hover {background-position: 4px -62px;}
section.arrow_right i:hover {background-position: 4px -1px;}

