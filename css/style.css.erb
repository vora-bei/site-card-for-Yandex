    /* Кнопки */
.button { display:inline-block;font-size:0.9em;line-height:	18px;
    padding:1px 7px 3px;
    margin:10px 5px;
    margin-right:0;
    border:1px solid #575757;

    background: #f2f2f2;
    background:-moz-linear-gradient(top, #f2f2f2 0,#DBDBDB 50%,#C7C7C7 60%);
    background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,#ededed),color-stop(44%,#d1d1d1),color-stop(100%,#aeaeae));
    background:-webkit-linear-gradient(top,#ededed 0,#d1d1d1 44%,#c0c0c0 100%);
    background:-o-linear-gradient(top,#ededed 0,#d1d1d1 44%,#c0c0c0 100%);
    background:-ms-linear-gradient(top,#ededed 0,#d1d1d1 44%,#c0c0c0 100%);
    background:linear-gradient(top,#ededed 0,#d1d1d1 44%,#c0c0c0 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed',endColorstr='#c0c0c0',GradientType=0);

    color: 	#222;
    text-align:center;
   /* text-shadow:0 1px 0 rgba(55,121,188,.5);     */
    text-decoration:none!important;
    vertical-align:middle; zoom:1;*display:inline;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;-webkit-box-shadow:0 1px 0 rgba(0,0,0,.1),inset 0 1px 0 rgba(255,255,255,.5),inset 0 -1px 0 rgba(255,255,255,.3);-moz-box-shadow:0 1px 0 rgba(0,0,0,.1),inset 0 1px 0 rgba(255,255,255,.5),inset 0 -1px 0 rgba(255,255,255,.3);box-shadow:0 1px 0 rgba(0,0,0,.1),inset 0 1px 0 rgba(255,255,255,.5),inset 0 -1px 0 rgba(255,255,255,.3)
}
