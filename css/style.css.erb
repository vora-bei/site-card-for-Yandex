.tablist th {padding: 5px; background:#ededed;
    background:-moz-linear-gradient(top, #ededed 0,#d1d1d1 44%,#c0c0c0 100%);
    background:-webkit-gradient(linear,left top,left bottom,color-stop(0%,#ededed),color-stop(44%,#d1d1d1),color-stop(100%,#c0c0c0));
    background:-webkit-linear-gradient(top,#ededed 0,#d1d1d1 44%,#c0c0c0 100%);
