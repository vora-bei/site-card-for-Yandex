.tablist {
    width: 100%;
    margin-bottom: 18px;
}
.tablist {
    border: 1px solid #dddddd;
    border-collapse: separate;
    *border-collapse: collapsed;
    border-left: 0;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}
.tablist th,
.tablist td {
    padding: 8px;
    line-height: 18px;
    text-align: left;
    vertical-align: top;
    border-top: 1px solid #dddddd;
}

.tablist tr:last-child td {border-bottom: 1px solid #DDDDDD;}
.tablist th:first-child, .tablist td:first-child {border-left: 1px solid #DDDDDD;}

.tablist caption + thead tr:first-child th,
.tablist caption + thead tr:first-child td,
.tablist colgroup + thead tr:first-child th,
.tablist colgroup + thead tr:first-child td,
.tablist thead:first-child tr:first-child th,
.tablist thead:first-child tr:first-child td {
    border-top: 1px solid #DDDDDD !important;
}


.tablist thead:first-child tr:first-child th:first-child,
.tablist tbody:first-child tr:first-child td:first-child {
    -webkit-border-top-left-radius: 4px;
    border-top-left-radius: 4px;
    -moz-border-radius-topleft: 4px;
}

.tablist thead:first-child tr:first-child th:last-child,
.tablist tbody:first-child tr:first-child td:last-child {
    -webkit-border-top-right-radius: 4px;
    border-top-right-radius: 4px;
    -moz-border-radius-topright: 4px;
}

.tablist thead:last-child tr:last-child th:first-child,
.tablist tbody:last-child tr:last-child td:first-child {
    -webkit-border-radius: 0 0 0 4px;
    -moz-border-radius: 0 0 0 4px;
    border-radius: 0 0 0 4px;
    -webkit-border-bottom-left-radius: 4px;
    border-bottom-left-radius: 4px;
    -moz-border-radius-bottomleft: 4px;
}

.tablist thead:last-child tr:last-child th:last-child,
.tablist tbody:last-child tr:last-child td:last-child {
    -webkit-border-bottom-right-radius: 4px;
    border-bottom-right-radius: 4px;
    -moz-border-radius-bottomright: 4px;
}

.tablist  tr:last-child td:last-child {
    -webkit-border-bottom-right-radius: 4px;
    border-bottom-right-radius: 4px;
    -moz-border-radius-bottomright: 4px;
}

